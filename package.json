{
    "name": "functional-cursors",
    "displayName": "Functional Cursors",
    "description": "Change your cursor content on the fly with any JavaScript",
    "version": "0.0.6",
    "publisher": "sporiley",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.jsCursors",
                "title": "jscursor"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "JavaScript Cursors Configuration",
            "properties": {
                "functional-cursors.functions": {
                    "type": "object",
                    "default": {}
                }
            }
        }
    },
    "repository": {
        "type": "github",
        "url": "https://github.com/stevengeeky/functional-cursors"
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "safe-eval": "^0.3.0"
    }
}
